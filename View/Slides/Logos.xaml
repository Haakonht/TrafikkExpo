<UserControl x:Class="TrafikkExpo.View.Slides.Logos"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
             xmlns:local="clr-namespace:TrafikkExpo.View.Slides"
             mc:Ignorable="d" 
             d:DesignHeight="1080" d:DesignWidth="1920">
    <Grid>
        <Grid.RowDefinitions>
            <RowDefinition Height="0.25*"/>
            <RowDefinition Height="0.50*"/>
            <RowDefinition Height="0.25*"/>
        </Grid.RowDefinitions>
        <Grid.ColumnDefinitions>
            <ColumnDefinition Width="0.15*"/>
            <ColumnDefinition Width="0.55*"/>
            <ColumnDefinition Width="0.30*"/>
        </Grid.ColumnDefinitions>
        <Grid x:Name="Schedule" Grid.Column="1" Grid.Row="1">
            <Image x:Name="rh" Source="/Resources/rh_inverted.png" Opacity="0"/>
            <Image x:Name="sem" Source="/Resources/sem_inverted.png" Opacity="0"/>
            <Image x:Name="bob" Source="/Resources/bob_inverted.png" Opacity="0"/>
        </Grid>
    </Grid>
</UserControl>
